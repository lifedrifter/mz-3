<?xml version='1.0' encoding='utf-8'?>
<widget id="com.microzaimy.web" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Займы онлайн</name>
    <description>
        Выбор подходящего займа для подачи заявки онлайн прямо с телефона
    </description>
    <author email="support@apps.cute.su">
        Aleksandr Gromov
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="26" />
    <preference name="orientation" value="portrait" />
</widget>
